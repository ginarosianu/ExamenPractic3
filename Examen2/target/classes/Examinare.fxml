        <!--&lt;!&ndash;<Insets/>&ndash;&gt;-->
        <!--&lt;!&ndash;</VBox.margin>&ndash;&gt;-->
        <!--</HBox>-->

        <HBox.margin>
            <Insets />
        </HBox.margin>

    </VBox>

</HBox>
