<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>


<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox alignment="center" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.MainController">
    <VBox spacing="20">
        <Label text="Options:"/>
        <Button fx:id="btnPersoana" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
                maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" onAction="#btnPersoanaClick"
                text="Examinator" textAlignment="JUSTIFY" textFill="#171616"/>
        <Button fx:id="btnExaminare" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
                maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" onAction="#btnExaminareClick"
                text="Examinare" textAlignment="JUSTIFY" textFill="#171616"/>
        <Button fx:id="btnRaport" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
                maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" onAction="#btnRaportClick"
                text="Raport" textAlignment="JUSTIFY" textFill="#171616"/>
    </VBox>

</HBox>